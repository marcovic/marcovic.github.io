  const currentScriptSrc = document.currentScript.src;

  const initialize = function () {
    const queryParamsString = currentScriptSrc.split("?")[1] || "";
    const queryParams = new URLSearchParams(queryParamsString);
    const isBeta = !!queryParams.get("beta");

    if (Math.random() < state.BETA_RATE && !isBeta && !window.Cypress) {
      const script = document.createElement("script");
      script.src = `${state.BETA_BASE_URL}/inline-payload/${state.FORM_ID}/faas-payload.js?${queryParamsString}&beta=true`;
      script.type = "text/javascript";
      script.defer = true;
      document.body.appendChild(script);
      return;
    }

    let formId = state.FORM_ID || queryParams.get("formId") || queryParams.get("formID");
    const formLayout = queryParams.get("formLayout");
    let formContainer =
      document.getElementById(queryParams.get("id") || "faas-form") ||
      (document.getElementById("__faas-form-app") || {}).parentElement;

    if (!formContainer) {
      return;
    }

    const html = state.RESPONSE;
    const frame = document.createElement("iframe");
    frame.setAttribute("title", "Inline Form");
    frame.setAttribute("src", "about:blank");
    frame.setAttribute("id", `faas-inline-form-${formId}`);
    frame.setAttribute("style", "border: 0; width: 100%; display: none; max-width: none; position: absolute;");
    if (formLayout) {
      frame.setAttribute("data-form-layout", formLayout);
    }
    frame.addEventListener("load", function () {
      frame.contentDocument.body.innerHTML = html;
      fixScriptTagsNotExecuting(frame.contentDocument.body);
